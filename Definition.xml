<Shapes xmlns="http://www.evolus.vn/Namespace/Pencil"
    xmlns:p="http://www.evolus.vn/Namespace/Pencil"
    xmlns:svg="http://www.w3.org/2000/svg"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    id="ysirotenko-pencil-collection-1"
    displayName="KaIOS Components"
    description="Components for design apps for mobile operation system KaiOS"
    author="Yuriy Sirotenko"
    url="https://ysirotenko.com">
<!--Your shapes go here -->
    <Properties>
        <PropertyGroup name="Header">
            <Property name="strokeColor" type="Color" displayName="Default stroke color">#2A0361</Property>
            <Property name="box" type="Dimension" displayName="Demension">240,26</Property>
            <Property name="strokeStyle" type="StrokeStyle" displayName="Stroke style">0|</Property>
            <Property name="fillColor" displayName="Background Color" type="Color">#2A0361</Property>
        </PropertyGroup>
    </Properties>

    
    <Shape id="KaiHeader" displayName="Header" icon="icons/Header-Purple.png">
        <Properties>
            <PropertyGroup name="Header">
                <Property name="strokeColor" type="Color" displayName="Stroce color">
                    <E>$$strokeColor</E>
                </Property>

                <Property name="box" type="Dimension" displayName="Demension">
                    <E>$$box</E>
                </Property>

                <Property name="strokeStyle" type="StrokeStyle" displayName="Stroke style">
                    <E>$$strokeStyle</E>
                </Property>

                <Property name="fillColor" displayName="Background Color" type="Color">
                    <E>$$fillColor</E>
                </Property>
            </PropertyGroup>

            <PropertyGroup name="HeaderText">
                <Property name="textContent" displayName="Content" type="PlainText"><![CDATA[Header]]></Property>
                <Property name="textColor" displayName="Color" type="Color">#ffffff</Property>
                <Property name="textAlign" displayName="Alignment" type="Alignment">1,1</Property>
                <Property name="textFont" displayName="Text Font" type="Font">Open Sans|400|normal|17px</Property> 
	       </PropertyGroup>
        </Properties>

        <Behaviors>
            <For ref="myRect">
                <Box>$box</Box>
                <StrokeColor>$strokeColor</StrokeColor>
                <StrokeStyle>$strokeStyle</StrokeStyle>
                <Fill>$fillColor</Fill>
            </For>
            <For ref="text">
                <Font>$textFont</Font>
                <TextContent>$textContent</TextContent>
                <Color>$textColor</Color>
                <BoxFit>
                    <Arg>Bound.fromBox($box, $box.w / 2, $box.h / 2);</Arg>
                    <Arg>$textAlign</Arg>
                </BoxFit>
            </For>
        </Behaviors>

        <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
            <rect id="myRect"/>
            <text id="text"/>
        </p:Content>
    </Shape>

    <Shape id="KaiTabs" displayName="Tabs" icon="icons/Tabs.png">
        <Properties>
            <PropertyGroup name="Tabs">
                <Property name="strokeColor" type="Color" displayName="Stroce color">#ffffff</Property>
                <Property name="box" type="Dimension" displayName="Demension">240,30</Property>
                <Property name="strokeStyle" type="StrokeStyle" displayName="Stroke style">
                    <E>$$strokeStyle</E>
                </Property>
                <Property name="fillColor" displayName="Background Color" type="Color">#ffffff</Property>
            </PropertyGroup>
            <PropertyGroup name="TabText">
                <Property name="textContent1" displayName="Content" type="PlainText"><![CDATA[Tab One]]></Property>
                <Property name="textContent2" displayName="Content" type="PlainText"><![CDATA[Tab Two]]></Property>
                <Property name="textContent3" displayName="Content" type="PlainText"><![CDATA[Tab Three]]></Property>
                <Property name="textColor" displayName="Color" type="Color">#6A6A6A</Property>
                <Property name="textAlign" displayName="Alignment" type="Alignment">0,1</Property>
                <Property name="textFont" displayName="Text Font" type="Font">Open Sans|400|normal|14px</Property> 
                <Property name="selectedBox" type="Dimension" displayName="Demension">50,3</Property>
                <Property name="fillSelectedColor" displayName="Selected Tab Color" type="Color">#2A0361</Property>
           </PropertyGroup>
        </Properties>
            <Behaviors>
                <For ref="myRect">
                    <Box>$box</Box>
                    <StrokeColor>$strokeColor</StrokeColor>
                    <StrokeStyle>$strokeStyle</StrokeStyle>
                    <Fill>$fillColor</Fill>
                </For>
                <For ref="tab1Text">
                    <Font>$textFont</Font>
                    <TextContent>$textContent1</TextContent>
                    <Color>$textColor</Color>
                    <BoxFit>
                        <Arg>Bound.fromBox($box, 10, $box.h / 2);</Arg>
                        <Arg>$textAlign</Arg>
                    </BoxFit>
                </For>
                <For ref="tab2Text">
                    <Font>$textFont</Font>
                    <TextContent>$textContent2</TextContent>
                    <Color>$textColor</Color>
                    <BoxFit>
                        <Arg>Bound.fromBox($box, 80, $box.h / 2);</Arg>
                        <Arg>$textAlign</Arg>
                    </BoxFit>
                </For>
                <For ref="tab3Text">
                    <Font>$textFont</Font>
                    <TextContent>$textContent3</TextContent>
                    <Color>$textColor</Color>
                    <BoxFit>
                        <Arg>Bound.fromBox($box, 150, $box.h / 2);</Arg>
                        <Arg>$textAlign</Arg>
                    </BoxFit>
                </For>
                <For ref="selectedBorder">
                    <Box>$selectedBox</Box>
                    <Fill>$fillSelectedColor</Fill>
                    <Bound>new Bound(80, $box.h - 5, $selectedBox.w, $selectedBox.h)</Bound>
                </For>
        </Behaviors>
        <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
            <rect id="myRect"/>
            <text id="tab1Text"/>
            <text id="tab2Text"/>
            <text id="tab3Text"/>
            <rect id="selectedBorder"/>
        </p:Content>
    </Shape>

    <Shape id="KaiSoftKeys" displayName="Soft Keys" icon="icons/SoftKeys.png">
         <Properties>
            <PropertyGroup name="SoftKeys">
                <Property name="strokeColor" type="Color" displayName="Stroce color">#CCCCCC</Property>
                <Property name="box" type="Dimension" displayName="Demension">240,30</Property>
                <Property name="strokeStyle" type="StrokeStyle" displayName="Stroke style">
                    <E>$$strokeStyle</E>
                </Property>
                <Property name="fillColor" displayName="Background Color" type="Color">#CCCCCC</Property>
                <Property name="textLeft" displayName="Content" type="PlainText"><![CDATA[Left Key]]></Property>
                <Property name="textSelect" displayName="Content" type="PlainText"><![CDATA[SELECT]]></Property>
                <Property name="textRight" displayName="Content" type="PlainText"><![CDATA[Right Key]]></Property>
                <Property name="textFont" displayName="Text Font" type="Font">Open Sans|600|normal|14px</Property> 
                <Property name="textColor" displayName="Color" type="Color">#323232</Property>
            </PropertyGroup>
        </Properties>
        <Behaviors>
            <For ref="myRect">
                <Box>$box</Box>
                <StrokeColor>$strokeColor</StrokeColor>
                <StrokeStyle>$strokeStyle</StrokeStyle>
                <Fill>$fillColor</Fill>
            </For>
            <For ref="LeftText">
                <Font>$textFont</Font>
                <TextContent>$textLeft</TextContent>
                <Color>$textColor</Color>
                <BoxFit>
                    <Arg>Bound.fromBox($box, 10, $box.h / 2);</Arg>
                    <Arg>new Alignment(0,1)</Arg>
                </BoxFit>
            </For>
            <For ref="SelectText">
                <Font>$textFont</Font>
                <TextContent>$textSelect</TextContent>
                <Color>$textColor</Color>
                <BoxFit>
                    <Arg>Bound.fromBox($box, 0, $box.h / 2);</Arg>
                    <Arg>new Alignment(1,1)</Arg>
                </BoxFit>
            </For>
            <For ref="RightText">
                <Font>$textFont</Font>
                <TextContent>$textRight</TextContent>
                <Color>$textColor</Color>
                <BoxFit>
                    <Arg>Bound.fromBox($box, 10, $box.h / 2);</Arg>
                    <Arg>new Alignment(2,1)</Arg>
                </BoxFit>
            </For>
        </Behaviors>
        <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
            <rect id="myRect"/>
            <text id="LeftText"/>
            <text id="SelectText"/>
            <text id="RightText"/>
        </p:Content>
    </Shape>

    <Shape id="KaiButton" displayName="Button" icon="icons/Button.png">
        <Properties>
            <PropertyGroup name="ButtonDefault">
                <Property name="strokeColor" type="Color" displayName="Stroce color">#8C8C8C</Property>
                <Property name="box" type="Dimension" displayName="Demension">220,36</Property>
                <Property name="strokeStyle" type="StrokeStyle" displayName="Stroke style">StrokeStyle.fromString("1|[1,1]");</Property>
                <Property name="fillColor" displayName="Background Color" type="Color">#FFFFFF</Property>
                <Property name="caption" displayName="Caption" type="PlainText"><![CDATA[Button]]></Property>
                <Property name="captionFont" displayName="Caption Font" type="Font">Open Sans|400|normal|17px</Property>
                <Property name="captionColor" displayName="Caption Color" type="Color">#323232</Property>
            </PropertyGroup>
        </Properties>

        <Behaviors>
            <For ref="myRect">
                <Box>$box</Box>
                <StrokeColor>$strokeColor</StrokeColor>
                <StrokeStyle>$strokeStyle</StrokeStyle>
                <Fill>$fillColor</Fill>
                <Radius>
                    <Arg>18</Arg>
                    <Arg>18</Arg>
                </Radius>
            </For>
            <For ref="caption">
                <Font>$captionFont</Font>
                <TextContent>$caption</TextContent>
                <Color>$captionColor</Color>
                <BoxFit>
                    <Arg>Bound.fromBox($box, 0, $box.h / 2);</Arg>
                    <Arg>new Alignment(1,1)</Arg>
                </BoxFit>
            </For>
        </Behaviors>

        <Actions>
            <Action id="selected" displayName="Make selected">
                <Impl>
                    var captionColor = this.getProperty("captionColor");
                    captionColor = Color.fromString("#FFFFFF");
                    this.setProperty("captionColor", captionColor);

                    var fillColor = this.getProperty("fillColor");
                    fillColor = Color.fromString("#8000FF");
                    this.setProperty("fillColor", fillColor);

                    var strokeStyle = this.getProperty("strokeStyle");
                    strokeStyle = StrokeStyle.fromString("0|");
                    this.setProperty("strokeStyle", strokeStyle);
                </Impl>
            </Action>

            <Action id="unselected" displayName="Make unselected">
                <Impl>
                    var captionColor = this.getProperty("captionColor");
                    captionColor = Color.fromString("#323232");
                    this.setProperty("captionColor", captionColor);

                    var fillColor = this.getProperty("fillColor");
                    fillColor = Color.fromString("#FFFFFF");
                    this.setProperty("fillColor", fillColor);

                    var strokeStyle = this.getProperty("strokeStyle");
                    strokeStyle = StrokeStyle.fromString("1|[1,1]");
                    this.setProperty("strokeStyle", strokeStyle);
                </Impl>
            </Action>
        </Actions>
        <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
            <rect id="myRect"/>
            <text id="caption"/>
        </p:Content>
    </Shape>

    <Shape id="KaiInput" displayName="Input" icon="icons/Input.png">
        <Properties>
            <PropertyGroup name="ButtonDefault">
                <Property name="strokeColor" type="Color" displayName="Stroce color">#AAAAAA</Property>
                <Property name="box" type="Dimension" displayName="Demension">220,26</Property>
                <Property name="strokeStyle" type="StrokeStyle" displayName="Stroke style">StrokeStyle.fromString("1|[1,1]");</Property>
                <Property name="fillColor" displayName="Background Color" type="Color">#FFFFFF</Property>
                <Property name="text" displayName="Caption" type="PlainText"><![CDATA[Input hint]]></Property>
                <Property name="textFont" displayName="Caption Font" type="Font">Open Sans|400|normal|14px</Property>
                <Property name="textColor" displayName="Caption Color" type="Color">#AAAAAA</Property>
            </PropertyGroup>
        </Properties>

        <Behaviors>
            <For ref="myRect">
                <Box>$box</Box>
                <StrokeColor>$strokeColor</StrokeColor>
                <StrokeStyle>$strokeStyle</StrokeStyle>
                <Fill>$fillColor</Fill>
                <Radius>
                    <Arg>2</Arg>
                    <Arg>2</Arg>
                </Radius>
            </For>
            <For ref="text">
                <Font>$textFont</Font>
                <TextContent>$text</TextContent>
                <Color>$textColor</Color>
                <BoxFit>
                    <Arg>Bound.fromBox($box, 10, $box.h / 2);</Arg>
                    <Arg>new Alignment(0,1)</Arg>
                </BoxFit>
            </For>
        </Behaviors>

        <Actions>
            <Action id="Typed" displayName="Make type">
                <Impl>
                    var textColor = this.getProperty("textColor");
                    textColor = Color.fromString("#323232");
                    this.setProperty("textColor", textColor);

                    var text = this.getProperty("text");
                    text = PlainText.fromString("Input text|");
                    this.setProperty("text", text);
                </Impl>
            </Action>
        </Actions>

        <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
            <rect id="myRect"/>
            <text id="text"/>
        </p:Content>
    </Shape>

    <Shape id="KaiRadio" displayName="Radio Button" icon="icons/Radio.png">
        <Properties>
            <PropertyGroup name="RadioDefault">
                <Property name="radio" type="Dimension" displayName="Demension">22,22</Property>
                <Property name="radioStyle" type="StrokeStyle" displayName="Stroke style">2|</Property>
                <Property name="radioStrokeColor" type="Color" displayName="Stroce color">#8C8C8C</Property>
                <Property name="radioFillColor" displayName="Radio Color" type="Color">#FFFFFF</Property>
                <Property name="radio2" type="Dimension" displayName="Demension">12,12</Property>
                <Property name="radio2Style" type="StrokeStyle" displayName="Stroke style">0|</Property>
                <Property name="radio2StrokeColor" type="Color" displayName="Stroce color">#8C8C8C</Property>
                <Property name="radio2FillColor" displayName="Background Color" type="Color">#8C8C8C</Property>
                <Property name="radio2Visibility" displayName="Radio visibility" type="Bool">true</Property>  
                <Property name="strokeColor" type="Color" displayName="Stroce color">#8C8C8C</Property>
                <Property name="box" type="Dimension" displayName="Demension">240,60</Property>
                <Property name="strokeStyle" type="StrokeStyle" displayName="Stroke style">1|[1,1];</Property>
                <Property name="fillColor" displayName="Background Color" type="Color">#FFFFFF</Property>
                <Property name="primaryText" displayName="Primary Text" type="PlainText"><![CDATA[Primary text]]></Property>
                <Property name="primaryTextFont" displayName="Primary Text Font" type="Font">Open Sans|400|normal|17px</Property>
                <Property name="primaryTextColor" displayName="Primary Text Color" type="Color">#323232</Property>
                <Property name="secondaryText" displayName="Secondary Text" type="PlainText"><![CDATA[Secondary text]]></Property>
                <Property name="secondaryTextFont" displayName="Secondary Text Font" type="Font">Open Sans|400|normal|14px</Property>
                <Property name="secondaryTextColor" displayName="Secondary Text Color" type="Color">#6A6A6A</Property>
            </PropertyGroup>
        </Properties>

        <Behaviors>
            <For ref="myRect">
                <Box>$box</Box>
                <StrokeColor>$strokeColor</StrokeColor>
                <StrokeStyle>$strokeStyle</StrokeStyle>
                <Fill>$fillColor</Fill>
            </For>
            <For ref="primaryText">
                <Font>$primaryTextFont</Font>
                <TextContent>$primaryText</TextContent>
                <Color>$primaryTextColor</Color>
                <BoxFit>
                    <Arg>Bound.fromBox($box, 10, 0);</Arg>
                    <Arg>new Alignment(0,0)</Arg>
                </BoxFit>
            </For>
            <For ref="secondaryText">
                <Font>$secondaryTextFont</Font>
                <TextContent>$secondaryText</TextContent>
                <Color>$secondaryTextColor</Color>
                <BoxFit>
                    <Arg>Bound.fromBox($box, 10, 15);</Arg>
                    <Arg>new Alignment(0,2)</Arg>
                </BoxFit>
            </For>
            <For ref="radio">
                <Box>$radio</Box>
                <StrokeColor>$radioStrokeColor</StrokeColor>
                <StrokeStyle>$radioStyle</StrokeStyle>
                <Radius>
                    <Arg>11</Arg>
                    <Arg>11</Arg>
                </Radius>
                <Bound>new Bound(($box.w - $radio.w) - 10, ($box.h / 2) - ($radio.h / 2), $radio.w, $radio.h)</Bound>
                <Fill>$radioFillColor</Fill>
            </For>
            <For ref="radio2">
                <Box>$radio2</Box>
                <StrokeColor>$radio2StrokeColor</StrokeColor>
                <StrokeStyle>$radio2Style</StrokeStyle>
                <Radius>
                    <Arg>11</Arg>
                    <Arg>11</Arg>
                </Radius>
                <Bound>new Bound(((($box.w - $radio.w) + ($radio2.w / 2))) - 11, (($box.h / 2) - ($radio.h / 2) + ($radio2.h / 2)) - 1, $radio2.w, $radio2.h)</Bound>
                <Fill>$radio2FillColor</Fill>
                <Visibility>$radio2Visibility</Visibility>
            </For>
        </Behaviors>
        <Actions>
            <Action id="RadioEnable" displayName="Enable">
                <Impl>
                    var fillColor = this.getProperty("fillColor");
                    fillColor = Color.fromString("#FFFFFF");;
                    this.setProperty("fillColor", fillColor);

                    var primaryTextColor = this.getProperty("primaryTextColor");
                    primaryTextColor = Color.fromString("#323232");;
                    this.setProperty("primaryTextColor", primaryTextColor);

                    var secondaryTextColor = this.getProperty("secondaryTextColor");
                    secondaryTextColor = Color.fromString("#6A6A6A");;
                    this.setProperty("secondaryTextColor", secondaryTextColor);

                    var radioStrokeColor = this.getProperty("radioStrokeColor");
                    radioStrokeColor = Color.fromString("#8C8C8C");;
                    this.setProperty("radioStrokeColor", radioStrokeColor);

                    var radioFillColor = this.getProperty("radioFillColor");
                    radioFillColor = Color.fromString("#FFFFFF");;
                    this.setProperty("radioFillColor", radioFillColor);

                    var radio2FillColor = this.getProperty("radio2FillColor");
                    radio2FillColor = Color.fromString("#8C8C8C");;
                    this.setProperty("radio2FillColor", radio2FillColor);

                    var radio2Visibility = this.getProperty("radio2Visibility");
                    radio2Visibility = true;
                    this.setProperty("radio2Visibility", radio2Visibility);
                </Impl>
            </Action>
            <Action id="RadioDisale" displayName="Disable">
                <Impl>
                    var fillColor = this.getProperty("fillColor");
                    fillColor = Color.fromString("#FFFFFF");;
                    this.setProperty("fillColor", fillColor);

                    var primaryTextColor = this.getProperty("primaryTextColor");
                    primaryTextColor = Color.fromString("#323232");;
                    this.setProperty("primaryTextColor", primaryTextColor);

                    var secondaryTextColor = this.getProperty("secondaryTextColor");
                    secondaryTextColor = Color.fromString("#6A6A6A");;
                    this.setProperty("secondaryTextColor", secondaryTextColor);

                    var radioStrokeColor = this.getProperty("radioStrokeColor");
                    radioStrokeColor = Color.fromString("#8C8C8C");;
                    this.setProperty("radioStrokeColor", radioStrokeColor);

                    var radioFillColor = this.getProperty("radioFillColor");
                    radioFillColor = Color.fromString("#FFFFFF");;
                    this.setProperty("radioFillColor", radioFillColor);

                    var radio2FillColor = this.getProperty("radio2FillColor");
                    radio2FillColor = Color.fromString("#8C8C8C");;
                    this.setProperty("radio2FillColor", radio2FillColor);

                    var radio2Visibility = this.getProperty("radio2Visibility");
                    radio2Visibility = false;
                    this.setProperty("radio2Visibility", radio2Visibility);
                </Impl>
            </Action>
            <Action id="RadioEnableSelected" displayName="Select and enable">
                <Impl>
                    var radio2Visibility = this.getProperty("radio2Visibility");
                    radio2Visibility = true;
                    this.setProperty("radio2Visibility", radio2Visibility);

                    var fillColor = this.getProperty("fillColor");
                    fillColor = Color.fromString("#8000FF");;
                    this.setProperty("fillColor", fillColor);

                    var primaryTextColor = this.getProperty("primaryTextColor");
                    primaryTextColor = Color.fromString("#FFFFFF");;
                    this.setProperty("primaryTextColor", primaryTextColor);

                    var secondaryTextColor = this.getProperty("secondaryTextColor");
                    secondaryTextColor = Color.fromString("#FFFFFF");;
                    this.setProperty("secondaryTextColor", secondaryTextColor);

                    var radioStrokeColor = this.getProperty("radioStrokeColor");
                    radioStrokeColor = Color.fromString("#FFFFFF");;
                    this.setProperty("radioStrokeColor", radioStrokeColor);

                    var radioFillColor = this.getProperty("radioFillColor");
                    radioFillColor = Color.fromString("#8000FF");;
                    this.setProperty("radioFillColor", radioFillColor);

                    var radio2FillColor = this.getProperty("radio2FillColor");
                    radio2FillColor = Color.fromString("#FFFFFF");;
                    this.setProperty("radio2FillColor", radio2FillColor);

                    var radio2Visibility = this.getProperty("radio2Visibility");
                    radio2Visibility = true;
                    this.setProperty("radio2Visibility", radio2Visibility);
                </Impl>
            </Action>

            <Action id="RadioDisableSelected" displayName="Select and disable">
                <Impl>
                    var radio2Visibility = this.getProperty("radio2Visibility");
                    radio2Visibility = true;
                    this.setProperty("radio2Visibility", radio2Visibility);

                    var fillColor = this.getProperty("fillColor");
                    fillColor = Color.fromString("#8000FF");;
                    this.setProperty("fillColor", fillColor);

                    var primaryTextColor = this.getProperty("primaryTextColor");
                    primaryTextColor = Color.fromString("#FFFFFF");;
                    this.setProperty("primaryTextColor", primaryTextColor);

                    var secondaryTextColor = this.getProperty("secondaryTextColor");
                    secondaryTextColor = Color.fromString("#FFFFFF");;
                    this.setProperty("secondaryTextColor", secondaryTextColor);

                    var radioStrokeColor = this.getProperty("radioStrokeColor");
                    radioStrokeColor = Color.fromString("#FFFFFF");;
                    this.setProperty("radioStrokeColor", radioStrokeColor);

                    var radioFillColor = this.getProperty("radioFillColor");
                    radioFillColor = Color.fromString("#8000FF");;
                    this.setProperty("radioFillColor", radioFillColor);

                    var radio2FillColor = this.getProperty("radio2FillColor");
                    radio2FillColor = Color.fromString("#FFFFFF");;
                    this.setProperty("radio2FillColor", radio2FillColor);

                    var radio2Visibility = this.getProperty("radio2Visibility");
                    radio2Visibility = false;
                    this.setProperty("radio2Visibility", radio2Visibility);
                </Impl>
            </Action>
        </Actions>
        <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
            <rect id="myRect"/>
            <text id="primaryText"/>
            <text id="secondaryText"/>
            <rect id="radio"/>
            <rect id="radio2"/>
        </p:Content>
    </Shape>
    <Shape id="KaiToast" displayName="Toast" icon="icons/Toast.png">
        <Properties>
            <PropertyGroup name="ToastDefault">
                <Property name="box" type="Dimension" displayName="Demension">240,36</Property>
                <Property name="strokeStyle" type="StrokeStyle" displayName="Stroke style">0|</Property>
                <Property name="fillColor" displayName="Background Color" type="Color">#323232</Property>
                <Property name="text" displayName="Text" type="PlainText"><![CDATA[This is a toast message.]]></Property>
                <Property name="textFont" displayName="Text Font" type="Font">Open Sans|400|normal|17px</Property>
                <Property name="textColor" displayName="Text Color" type="Color">#FFFFFF</Property>
            </PropertyGroup>
        </Properties>

        <Behaviors>
            <For ref="myRect">
                <Box>$box</Box>
                <StrokeStyle>$strokeStyle</StrokeStyle>
                <Fill>$fillColor</Fill>
            </For>
            <For ref="text">
                <Font>$textFont</Font>
                <TextContent>$text</TextContent>
                <Color>$textColor</Color>
                <BoxFit>
                    <Arg>Bound.fromBox($box, 0, 0);</Arg>
                    <Arg>new Alignment(1,1)</Arg>
                </BoxFit>
            </For>
        </Behaviors>
        <p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
            <filter id="shadow" x="0" y="0" width="200%" height="200%">
                <feDropShadow dx="0" dy="2.5" stdDeviation="3" flood-color="#000000" flood-opacity="0.5" />
            </filter>
            <rect id="myRect" filter="url(#shadow)"/>
            <text id="text"/>
        </p:Content>
    </Shape>


</Shapes>
